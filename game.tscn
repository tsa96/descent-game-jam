resource_name = "FPS Counter"
script/source = "extends Label

@onready var player := $\"../../../World/Player\" as Player

func _process(_delta: float) -> void:
	set_text(\"FPS %d \\nX: %d \\nY: %d \\n\" % [
		Engine.get_frames_per_second(),
		player.position.x,
		player.position.y
	])
"

